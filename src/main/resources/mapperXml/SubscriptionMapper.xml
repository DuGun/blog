<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.SubscriptionMapper">
  <resultMap id="BaseResultMap" type="bean.SubscriptionKey">
    <id column="email" jdbcType="VARCHAR" property="email" />
    <id column="article_id" jdbcType="VARCHAR" property="articleId" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="bean.SubscriptionKey">
    delete from subscription
    where email = #{email,jdbcType=VARCHAR}
      and article_id = #{articleId,jdbcType=VARCHAR}
  </delete>
  <insert id="insert" parameterType="bean.SubscriptionKey">
    insert into subscription (email, article_id)
    values (#{email,jdbcType=VARCHAR}, #{articleId,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="bean.SubscriptionKey">
    insert into subscription
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="email != null">
        email,
      </if>
      <if test="articleId != null">
        article_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="email != null">
        #{email,jdbcType=VARCHAR},
      </if>
      <if test="articleId != null">
        #{articleId,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>